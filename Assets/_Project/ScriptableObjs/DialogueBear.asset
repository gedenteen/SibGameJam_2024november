  phrases:
  - nameOfSpeaker: "\u0424\u0435\u044F"
    text: "-\u0412\u0430\u043C \u043D\u0443\u0436\u043D\u0430 \u043F\u043E\u043C\u043E\u0449\u044C?\r"
  - nameOfSpeaker: "\u041C\u0435\u0434\u0432\u0435\u0434\u044C"
    text: "-\u041F-\u043F\u043E\u043C\u043E\u0433\u0438\u0442\u0435, \u0438\u043A,
      \u043A\u0440\u0435\u0441\u0442\u044C\u044F\u043D\u0435.\r\n*\u041C\u0435\u0434\u0432\u0435\u0434\u044C
      \u043D\u0435\u0443\u043A\u043B\u044E\u0436\u0435 \u043F\u043E\u0434\u043D\u044F\u043B\u0441\u044F
      \u043D\u0430 \u043B\u0430\u043F\u044B, \u043F\u043E\u0448\u0430\u0442\u044B\u0432\u0430\u044F\u0441\u044C.*\r\n"
  - nameOfSpeaker: "\u041C\u0435\u0434\u0432\u0435\u0434\u044C"
    text: "-\u044F \u0438\u0437 \u0434\u043E\u043C\u0430 \u0431-\u0431\u0430\u0431\u044B
      \u042F\u0433\u0438, \u0438\u043A, \u043F\u0443\u0442\u044C \u0434\u0435\u0440\u0436\u0443,
      \u043D\u0435 \u043C\u043E\u0433\u0443 \u043C\u043E\u0441\u0442 \u043E\u043F\u0443\u0441\u0442\u0438\u0442\u044C\r"
  - nameOfSpeaker: "\u0424\u0435\u044F"
    text: "-\u0410 \u0432\u044B \u0437\u043D\u0430\u0435\u0442\u0435, \u043A\u0443\u0434\u0430
      \u0432\u0435\u0434\u0435\u0442 \u0442\u0440\u043E\u043F\u0438\u043D\u043A\u0430?\r\n-\u041A
      \u041A\u043E\u0449\u0435\u044E, \u044F...\r"
  - nameOfSpeaker: "\u041C\u0435\u0434\u0432\u0435\u0434\u044C"
    text: "*\u043F\u043B\u044E\u0445\u043D\u0443\u043B\u0441\u044F \u043D\u0430 \u0437\u0435\u043C\u043B\u044E*"
  - nameOfSpeaker: "\u041C\u0435\u0434\u0432\u0435\u0434\u044C"
    text: "...\u043F\u043E\u043C\u043E\u0433\u0438\u0442\u0435 \u043F\u0435\u0440\u0435\u0441\u0435\u0447\u044C
      \u043C\u043E\u0441\u0442, \u044F \u0432\u0430\u0441 \u043E\u0442\u0431\u043B\u0430\u0433\u043E\u0434\u0430\u0440\u044E,
      \u0438\u043A, \u0434-\u0434\u0443\u0448\u0430 \u0443 \u043C\u0435\u043D\u044F
      \u0449\u0435\u0434\u0440\u0430\u044F!"
